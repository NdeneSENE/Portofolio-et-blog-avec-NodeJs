<%- include("partials/header") -%>
<h1>Mes messages</h1>

<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">Nom & Prenom</th>
      <th scope="col">Email</th>
      <th scope="col">Object</th>
    </tr>
  </thead>
  <tbody>
  <% Message.forEach(function(message){ %>
    <tr>
      <td><%= message.nom %></td>
      <td><%= message.email %></td>
      <td><%= message.sujet %></td>
    </tr>
  <% }); %>

  </tbody>
</table>

<%- include("partials/footer") -%>